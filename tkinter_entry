from tkinter import *
root = Tk()

e = Entry(root, width= 50)
e.pack()
e.insert(0,"Enter your Name: ")

def myclick():
    mylabel1= Label(root,text='Hello'+ e.get())
    mylabel1.pack()

myButton = Button(root, text = 'Enter your Name',command=myclick)
myButton.pack()

root.mainloop()